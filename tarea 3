from igraph import *
from numpy import *
from matplotlib import *
from json import loads
with open("CIAC.json") as of:
        data=loads(of.readline())
comentarios=[]
like=[]
usuariosL=[]
usuariosC=[]

for i in data:
    for k,o in i.items():
            if k=="likes":
                temp=o["data"]
                for j in temp:
                    temp3=j["id"]
                    usuariosL.append(temp3)
                    like.append(temp3)
            elif k == "comments":
                temp2=o["data"]
                for g in temp2:
                    temp4=g["from"]
                    temp5 = temp4.items()
                    for x in temp5:
                    	if x[0] == "id":
                    		usuariosC.append(x[1])
                    	else:
                    		continue                  
set(usuariosL)
set(usuariosC)

linea marciana de prueba

